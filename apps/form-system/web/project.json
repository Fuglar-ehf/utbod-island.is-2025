{
  "name": "form-system-web",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/form-system/web/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/form-system/web",
        "index": "apps/form-system/web/src/index.html",
        "baseHref": "/form/",
        "main": "apps/form-system/web/src/main.tsx",
        "tsConfig": "apps/form-system/web/tsconfig.app.json",
        "assets": [
          "apps/form-system/web/src/favicon.ico",
          "apps/form-system/web/src/assets"
        ],
        "styles": [
          "apps/form-system/web/src/styles.scss"
        ],
        "scripts": [],
        "webpackConfig": "apps/form-system/web/webpack.config.js",
        "resolve": {
          "alias": {
            "browserslist": "browserslist/index.js"
          }
        }
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/form-system/web/src/environments/environment.ts",
              "with": "apps/form-system/web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "form-system-web:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "form-system-web:build:development"
        },
        "production": {
          "buildTarget": "form-system-web:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/form-system/web/jest.config.ts"
      }
    }
  }
}