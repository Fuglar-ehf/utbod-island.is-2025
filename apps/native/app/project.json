{
  "root": "apps/native/app",
  "sourceRoot": "apps/native/app/src",
  "projectType": "application",
  "generators": {},
  "targets": {
    "script": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/native/app",
        "command": "yarn {args.name}"
      }
    },
    "schemas/codegen": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "parallel": false,
        "command": "graphql-codegen --config apps/native/app/codegen.yml"
      }
    }
  }
}
