{
  "name": "services-university-gateway-backend",
  "sourceRoot": "apps/services/university-gateway/backend/src",
  "projectType": "application",
  "prefix": "services-university-gateway-backend",
  "generators": {},
  "targets": {
    "build": {
      "executor": "./tools/executors/node:build",
      "options": {
        "outputPath": "dist/apps/services/university-gateway/backend",
        "main": "apps/services/university-gateway/backend/src/main.ts",
        "tsConfig": "apps/services/university-gateway/backend/tsconfig.app.json",
        "assets": [
          {
            "glob": "*",
            "input": "apps/services/university-gateway/backend/migrations",
            "output": "./migrations"
          },
          {
            "glob": "*",
            "input": "apps/services/university-gateway/backend/seeders",
            "output": "./seeders"
          },
          {
            "glob": ".sequelizerc",
            "input": "apps/services/university-gateway/backend",
            "output": "./"
          },
          {
            "glob": "sequelize.config.js",
            "input": "apps/services/university-gateway/backend",
            "output": "./"
          }
        ],
        "maxWorkers": 2
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "services-university-gateway-backend:build",
        "runtimeArgs": ["--no-experimental-fetch", "--trace-deprecation"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/services/university-gateway/backend/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "dev-services": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose up -d",
        "cwd": "apps/services/university-gateway/backend"
      }
    },
    "schemas/build-openapi": {
      "executor": "nx:run-commands",
      "options": {
        "outputPath": "apps/services/university-gateway/backend/src/openapi.yaml",
        "command": "cross-env INIT_SCHEMA=true yarn ts-node -P apps/services/university-gateway/backend/tsconfig.app.json apps/services/university-gateway/backend/src/buildOpenApi.ts"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sequelize-cli db:migrate",
        "cwd": "apps/services/university-gateway/backend"
      }
    },
    "migrate/undo": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sequelize-cli db:migrate:undo",
        "cwd": "apps/services/university-gateway/backend"
      }
    },
    "migrate/generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sequelize-cli migration:generate --name $(whoami)",
        "cwd": "apps/services/university-gateway/backend"
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "sequelize-cli db:seed:all",
        "cwd": "apps/services/university-gateway/backend"
      }
    },
    "docker-express": {},
    "dev-init": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn dev-services services-university-gateway-backend",
          "yarn nx run services-university-gateway-backend:migrate",
          "yarn nx run services-university-gateway-backend:seed"
        ],
        "parallel": false
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["yarn start services-university-gateway-backend"],
        "parallel": true
      }
    }
  },
  "tags": ["scope:nest-TODOx"]
}
