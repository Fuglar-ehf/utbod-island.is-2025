{
  "name": "application-templates-old-age-pension",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/application/templates/old-age-pension/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/application/templates/old-age-pension/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/application/templates/old-age-pension"
      ],
      "options": {
        "jestConfig": "libs/application/templates/old-age-pension/jest.config.js",
        "passWithNoTests": true
      }
    },
    "extract-strings": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn ts-node -P libs/localization/tsconfig.lib.json libs/localization/scripts/extract 'libs/application/templates/old-age-pension/src/lib/messages.ts'"
      }
    }
  },
  "tags": ["scope:application-system", "lib:application-system"]
}
