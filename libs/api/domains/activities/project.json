{
  "root": "libs/api/domains/activities",
  "sourceRoot": "libs/api/domains/activities/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/domains/activities/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api/domains/activities"],
      "options": {
        "jestConfig": "libs/api/domains/activities/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["lib:api", "scope:api"]
}
