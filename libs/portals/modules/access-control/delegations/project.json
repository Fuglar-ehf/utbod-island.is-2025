{
  "root": "libs/portals/modules/access-control/delegations",
  "sourceRoot": "libs/portals/modules/access-control/delegations/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/portals/modules/access-control/delegations/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/portals/modules/access-control/delegations"
      ],
      "options": {
        "jestConfig": "libs/portals/modules/access-control/delegations/jest.config.js",
        "passWithNoTests": true
      }
    },
    "extract-strings": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn ts-node -P libs/localization/tsconfig.lib.json libs/localization/scripts/extract 'libs/portals/modules/access-control/delegations/src/{components,screens,hooks}/**/*.{js,ts,tsx}'"
      }
    }
  },
  "tags": [
    "lib:react"
  ]
}
