{
  "name": "island-ui-theme",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/island-ui/theme/src",
  "projectType": "library",
  "tags": ["lib:react", "scope:react"],
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/island-ui/theme/jest.config.ts"
      },
      "outputs": ["{workspaceRoot}/coverage/libs/island-ui/theme"]
    },
    "run-figma-tokens-mapper": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "node libs/island-ui/theme/src/lib/tokens/figma-tokens-mapper.js",
          "prettier --write libs/island-ui/theme/src/lib/tokens/figmaPrimitives.ts",
          "prettier --write libs/island-ui/theme/src/lib/tokens/figmaColors.ts",
          "prettier --write libs/island-ui/theme/src/lib/tokens/figmaTypography.ts"
        ]
      }
    }
  }
}
