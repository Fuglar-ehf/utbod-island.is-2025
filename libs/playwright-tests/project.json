{
  "name": "playwright-tests",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/playwright-tests/src",
  "tags": ["lib:react", "scope:react"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/playwright-tests",
        "main": "libs/playwright-tests/src/index.ts",
        "tsConfig": "libs/playwright-tests/tsconfig.lib.json",
        "assets": ["libs/playwright-tests/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/playwright-tests/jest.config.ts"
      }
    }
  }
}
