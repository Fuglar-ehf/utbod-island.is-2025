{
  "name": "clients-contentful-graphql",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/contentful-graphql/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/clients/contentful-graphql/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/clients/contentful-graphql"],
      "options": {
        "jestConfig": "libs/clients/contentful-graphql/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "codegen/backend-client": {
      "executor": "nx:run-commands",
      "options": {
        "command": "graphql-codegen --config libs/clients/contentful-graphql/codegen.yaml"
      },
      "inputs": ["{projectRoot}/codegen.yaml"],
      "outputs": ["{projectRoot}/gen", "{projectRoot}/**/*.generated.ts"]
    }
  },
  "implicitDependencies": [],
  "tags": ["lib:client", "scope:client"]
}
