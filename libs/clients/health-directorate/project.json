{
  "name": "clients-health-directorate",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/health-directorate/src",
  "tags": ["scope:client", "lib:client"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/clients/health-directorate"],
      "options": {
        "jestConfig": "libs/clients/health-directorate/jest.config.ts"
      }
    },
    "update-openapi-document/organ-donation": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "curl -H 'X-Road-Client: IS-DEV/GOV/10000/island-is-client' http://localhost:8081/r1/IS-DEV/GOV/10015/EmbaettiLandlaeknis-Protected/getOpenAPI?serviceCode=landlaeknir > src/lib/clients/organ-donation/clientConfig.json",
          "prettier --write src/lib/clients/organ-donation/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/health-directorate/src/lib/clients/organ-donation"
      }
    },
    "codegen/backend-client": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn openapi-generator -o libs/clients/health-directorate/src/lib/clients/organ-donation/gen/fetch -i libs/clients/health-directorate/src/lib/clients/organ-donation/clientConfig.json",
          "yarn openapi-generator -o libs/clients/health-directorate/src/lib/clients/vaccinations/gen/fetch -i libs/clients/health-directorate/src/lib/clients/vaccinations/clientConfig.json",
          "yarn openapi-generator -o libs/clients/health-directorate/src/lib/clients/occupational-license/gen/fetch -i libs/clients/health-directorate/src/lib/clients/occupational-license/clientConfig.json"
        ]
      },
      "outputs": ["{projectRoot}/**/gen/fetch"]
    },
    "update-openapi-document/vaccinations": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "curl -H 'X-Road-Client: IS-DEV/GOV/10000/island-is-client' http://localhost:8081/r1/IS-DEV/GOV/10015/EmbaettiLandlaeknis-Protected/getOpenAPI?serviceCode=landlaeknir > src/lib/clients/vaccinations/clientConfig.json",
          "prettier --write src/lib/clients/vaccinations/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/health-directorate/src/lib/clients/vaccinations"
      }
    },
    "codegen/backend-client/vaccinations": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn openapi-generator -o libs/clients/health-directorate/src/lib/clients/vaccinations/gen/fetch -i libs/clients/health-directorate/src/lib/clients/vaccinations/clientConfig.json"
      },
      "outputs": ["{projectRoot}/**/gen/fetch"]
    },
    "update-openapi-document/occupational-license": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "curl -H 'X-Road-Client: IS-DEV/GOV/10000/island-is-client' http://localhost:8081/r1/IS-DEV/GOV/10015/EmbaettiLandlaeknis-Protected/getOpenAPI?serviceCode=landlaeknir > src/lib/clients/occupational-license/clientConfig.json",
          "prettier --write src/lib/clients/occupational-license/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/health-directorate/src/lib/clients/occupational-license"
      }
    },
    "codegen/backend-client/occupational-license": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn openapi-generator -o libs/clients/health-directorate/src/lib/clients/occupational-license/gen/fetch -i libs/clients/health-directorate/src/lib/clients/occupational-license/clientConfig.json"
      },
      "outputs": ["{projectRoot}/**/gen/fetch"]
    }
  }
}
