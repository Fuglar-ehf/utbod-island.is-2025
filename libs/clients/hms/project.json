{
  "root": "libs/clients/hms",
  "sourceRoot": "libs/clients/hms/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/clients/hms/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/clients/hms"],
      "options": {
        "jestConfig": "libs/clients/hms/jest.config.js",
        "passWithNoTests": true
      }
    },
    "schemas/external-openapi-generator": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn openapi-generator -o libs/clients/hms/gen/fetch -i libs/clients/hms/src/clientConfig.yaml"
      }
    }
  },
  "tags": []
}
