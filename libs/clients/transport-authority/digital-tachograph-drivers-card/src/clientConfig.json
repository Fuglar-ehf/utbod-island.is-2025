  "paths": {
    "/drivercards/{persidno}/newest": {
      "get": {
        "summary": "Newest driver card for a person",
        "tags": ["DriverCards"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NewestIcelandicDriverCardResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        },
        "operationId": "get-newesticelandicdrivercard",
        "description": "Returns the newest driver card for a person with icelandic persidno"
      },
      "parameters": [
        {
          "schema": {
            "type": "string",
            "pattern": "[0-9]+",
            "minLength": 10,
            "maxLength": 10
          },
          "name": "persidno",
          "in": "path",
          "required": true,
          "description": "Person id number"
        }
      ]
    },
    "/drivercards/tachonetcheck": {
      "post": {
        "summary": "Check TachoNet for issued cards",
        "operationId": "post-tachonetcheck",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TachonetCheckResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TachonetCheckRequest"
              }
            }
          }
        },
        "description": "Returns info on driver cards in TachoNet member states",
        "tags": ["TachoNet"]
      },
      "parameters": []
    },
    "/drivercards": {
      "post": {
        "summary": "Creates a new driver card application",
        "tags": ["DriverCards"],
        "operationId": "post-drivercards",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DriverCardApplicationResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DriverCardApplicationRequest"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/DriverCardApplicationRequest"
              }
            }
          },
          "description": ""
        },
        "description": "Registers a new driver card application"
      }
    },
    "/individual/{persidno}/photoandsignature": {
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "persidno",
          "in": "path",
          "required": true,
          "description": "Person id number"
        }
      ],
      "get": {
        "summary": "Newest newest photo and signature for an individual",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PhotoAndSignatureResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "401": {
            "$ref": "#/components/responses/Unauthorized"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "500": {
            "$ref": "#/components/responses/InternalServerError"
          }
        },
        "operationId": "get-individual-persidno-photoandsignature",
        "description": "Returns the newest newest photo and signature for a person with icelandic persidno",
        "tags": ["Individual"]
      }
    }
  },
