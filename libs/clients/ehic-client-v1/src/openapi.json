  "components": {
    "schemas": {
      "CardInfoDTO": {
        "type": "object",
        "properties": {
          "cardNumber": {
            "type": "string",
            "nullable": true
          },
          "expiryDate": {
            "$ref": "#/components/schemas/DateOnly"
          },
          "issued": {
            "$ref": "#/components/schemas/DateOnly"
          },
          "sent": {
            "$ref": "#/components/schemas/DateOnly"
          },
          "resent": {
            "$ref": "#/components/schemas/DateOnly"
          },
          "cardType": {
            "type": "string",
            "nullable": true
          },
          "sentStatus": {
            "type": "string",
            "nullable": true
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "isPlastic": {
            "type": "boolean",
            "readOnly": true
          },
          "isTemp": {
            "type": "boolean",
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "CardStatusResponseDTO": {
        "type": "object",
        "properties": {
          "applicantNationalId": {
            "type": "string",
            "nullable": true
          },
          "isInsured": {
            "type": "boolean",
            "nullable": true
          },
          "canApply": {
            "type": "boolean",
            "nullable": true
          },
          "cards": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CardInfoDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DateOnly": {
        "type": "object",
        "properties": {
          "year": {
            "type": "integer",
            "format": "int32"
          },
          "month": {
            "type": "integer",
            "format": "int32"
          },
          "day": {
            "type": "integer",
            "format": "int32"
          },
          "dayOfWeek": {
            "$ref": "#/components/schemas/DayOfWeek"
          },
          "dayOfYear": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "dayNumber": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "DayOfWeek": {
        "enum": [0, 1, 2, 3, 4, 5, 6],
        "type": "integer",
        "format": "int32"
      },
      "TempPDFCardResponseDTO": {
        "type": "object",
        "properties": {
          "data": {
            "type": "string",
            "format": "byte",
            "nullable": true
          },
          "fileName": {
            "type": "string",
            "nullable": true
          },
          "contentType": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}
