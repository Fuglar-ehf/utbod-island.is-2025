{
  "openapi": "3.0.1",
  "info": {
    "title": "Námskeið",
    "description": "Vefþjónusta til þess að fletta upp á og skrá í námskeið.",
    "contact": {
      "name": "Ut teymi vinnueftirlits",
      "email": "ut@vinnueftirlit.is"
    },
    "version": "v1",
    "x-pricing": "free",
    "x-category": "official",
    "x-links": {
      "responsibleParty ": "https://www.vinnueftirlit.is"
    }
  },
  "servers": [
    {
      "url": "/namskeid"
    }
  ],
  "paths": {
    "/api/Company": {
      "get": {
        "tags": ["Company"],
        "parameters": [
          {
            "name": "nationalId",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CompanyDTO"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/Course": {
      "get": {
        "tags": ["Course"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CourseDTO"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/Course/{courseId}": {
      "get": {
        "tags": ["Course"],
        "parameters": [
          {
            "name": "courseId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CourseDTO"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/api/Individual": {
      "get": {
        "tags": ["Individual"],
        "parameters": [
          {
            "name": "courseID",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "nationalIds",
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/IndividualDTO"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CompanyDTO": {
        "type": "object",
        "properties": {
          "nationalId": {
            "type": "string",
            "nullable": true
          },
          "mayPayWithAnAccount": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "CourseDTO": {
        "type": "object",
        "properties": {
          "alwaysOpen": {
            "type": "boolean"
          },
          "category": {
            "type": "string",
            "nullable": true
          },
          "dateFrom": {
            "type": "string",
            "nullable": true
          },
          "dateTo": {
            "type": "string",
            "nullable": true
          },
          "descriptionUrl": {
            "type": "string",
            "nullable": true
          },
          "location": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "integer",
            "format": "int32"
          },
          "registrationUrl": {
            "type": "string",
            "nullable": true
          },
          "courseId": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "subCategory": {
            "type": "string",
            "nullable": true
          },
          "time": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IndividualDTO": {
        "type": "object",
        "properties": {
          "nationalID": {
            "type": "string",
            "nullable": true
          },
          "mayTakeCourse": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ValidationProblemDetails": {
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Token",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": []
    }
  ]
}
