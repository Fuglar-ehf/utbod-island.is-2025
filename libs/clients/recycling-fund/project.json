{
  "name": "clients-recycling-fund",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/recycling-fund/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/clients/recycling-fund/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/clients/recycling-fund"],
      "options": {
        "jestConfig": "libs/clients/recycling-fund/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "codegen/backend-client": {
      "executor": "nx:run-commands",
      "options": {
        "command": "graphql-codegen --config libs/clients/recycling-fund/codegen.yaml"
      },
      "inputs": [
        "default",
        "{workspaceRoot}/apps/air-discount-scheme/api/src/api.graphql"
      ],
      "outputs": [
        "{projectRoot}/gen"
      ]
    }
  },
  "implicitDependencies": ["skilavottord-ws"],
  "tags": ["lib:client", "scope:client"]
}
